{
  "description": "log matching URLs",
  "manifest_version": 2,
  "name": "gpd",
  "version": "0.1",

  "permissions": [
    "<all_urls>", "webRequest"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "applications": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  }

}
